                        <!--
                        <Backdrop name="$(parent)Bg" centerColor="ff9900" edgeColor="000000" alpha="0.9">
                            <AnchorFill/>
                            <Edge edgeSize="1"/>
                        </Backdrop>
                        -->
                        <Label name="$(parent)Title" color="FFFFFF" font="BTGFont">
                            <Anchor offsetY="4" point="TOP" relativePoint="TOP" relativeTo="$(parent)"/>
                            <OnInitialized>self:SetText(&quot;|cFFAA33&quot;..BTG.name..&quot;|r &quot;..BTG.version)</OnInitialized>
